  "description": "Simple, efficient timestamps for symfony with doctrine",
  "keywords": ["doctrine", "timestamp"],
  "homepage": "http://github.com/incompass/timestampable-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Joe Mizzi",
      "email": "joe@casechek.com"
    }
  ],
  "require": {
    "php": ">=5.5.0",
