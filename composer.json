  },
  "require-dev": {
    "phpunit/phpunit": "^4.2|^5.0",
    "symfony/expression-language": "~2.6|~3.0",
    "symfony/yaml": "*",
    "symfony/browser-kit": "*",
    "symfony/class-loader": "*",
    "symfony/finder": "*",
    "symfony/css-selector": "*",
    "symfony/process": "*",
    "doctrine/doctrine-bundle": "*",
    "symfony/twig-bundle": "*",
